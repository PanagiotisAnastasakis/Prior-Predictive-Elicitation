    "def get_bernoulli_probs(partition, lam, covariate_set):\n",
    "\n",
    "    nom = jnp.inner(jnp.array(lam[:5]), covariate_set)\n",
    "    den = jnp.sqrt(1 + covariate_set.T@jnp.diag(jnp.array(lam[5:]))@covariate_set)\n",
    "\n",
    "    p1 = 1 - scs.norm.cdf(nom / den)\n",
    "\n",
    "    if partition == 0:\n",
    "        return p1\n",
    "\n",
    "    return 1 - p1\n",
    "\n",
    "\n",
    "\n",
    "\n",
